{
    "name": "getidtoken",
    "version": "1.0.0",
    "title": "Get ID Token Activity",
    "author": "TIBCO Software Inc.",
    "type": "flogo:activity",
     
    "display": {
       "category": "GCP",
       "visible": true,
       "smallIcon": "icons/gcp-getidtoken@2x.png",
       "largeIcon": "icons/gcp-getidtoken@3x.png",
       "description": "This activity gets an id token from gcp"
    },
 
    "ref": "github.com/mmussett/extensions/gcp/activity/getidtoken",
    "inputs": [
           {
            "name": "url",
            "type": "string",
            "required": true
           }
    ],
  
    "outputs": [
           {
            "name": "accessToken",
            "type": "string"
          },
          {
            "name": "tokenType",
            "type": "string"
          },
          {
            "name": "refreshToken",
            "type": "string"
          },
          {
            "name": "expiry",
            "type": "string"
          }
    ]
}